%p#notice= notice

%p
  %b City:
  = @location.city
%p
  %b County:
  = @location.state.name
%p
  %b Country:
  = @location.country
%p
  %b Zip:
  = @location.zip
%p
  %b Latitude:
  = @location.latitude
%p
  %b Longitude:
  = @location.longitude

= link_to 'Edit', edit_location_path(@location), class: "btn btn-brand-secondary btn-sm"
= link_to 'Back', locations_path, class: "btn btn-default btn-sm"
